{
  "name": "ngx-quiz-api",
  "private": true,
  "version": "0.0.1",
  "description": "Server side for ngx-quiz",
  "main": "server.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "node dist/server.js",
    "build": "tsc",
    "watch": "npm run build && tsc -w",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "lint-fix": "tslint --fix -c tslint.json --project tsconfig.json",
    "test": "mocha -r ts-node/register spec/**/*.ts",
    "oatts": "oatts generate -s ./swagger/swagger.json -w ./out-tests --host localhost:3333",
    "oatts-test": "mocha out-tests",
    "dev": "ts-node-dev src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmstss/ngx-quiz-api.git"
  },
  "keywords": [
    "node",
    "express",
    "mongoose",
    "mongodb",
    "api"
  ],
  "author": "Viachaslau Tyshkavets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pmstss/ngx-quiz-api/issues"
  },
  "homepage": "https://github.com/pmstss/ngx-quiz-api#readme",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.11",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.3.12",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.21",
    "@types/node-fetch": "^2.1.4",
    "bcrypt": "3.0.2",
    "body-parser": "^1.18.3",
    "chakram": "^1.5.0",
    "connect-mongo": "^2.0.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.4.0",
    "mocha": "^5.2.0",
    "mongoose": "^5.4.10",
    "morgan": "^1.9.1",
    "ngrx-quiz-common": "0.0.5",
    "node-fetch": "^2.3.0",
    "nodemailer": "^5.1.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.1"
  },
  "devDependencies": {
    "ts-node-dev": "^1.0.0-pre.32"
  }
}
